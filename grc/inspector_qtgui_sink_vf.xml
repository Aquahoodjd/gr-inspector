<block>
  <name>QT GUI Inspector Sink</name>
  <key>inspector_qtgui_sink_vf</key>
  <category>Inspector</category>
  <import>import inspector</import>
  <import>from PyQt4 import Qt</import>
  <import>from gnuradio import qtgui</import>
  <import>import sip</import>
  <make>#set $win = 'self._%s_win'%$id
inspector.qtgui_inspector_sink_vf($samp_rate, $fft_len, $cfreq, $rf_unit)
self._$(id)_win = sip.wrapinstance(self.$(id).pyqwidget(), Qt.QWidget)
$(gui_hint()($win))</make>
  <callback>set_rf_unit($rf_unit)</callback>
  <callback>set_cfreq($cfreq)</callback>
  <param>
    <name>Samp Rate</name>
    <key>samp_rate</key>
    <type>real</type>
  </param>
  <param>
    <name>FFT len</name>
    <key>fft_len</key>
    <type>int</type>
  </param>
  <param>
    <name>Center freq</name>
    <key>cfreq</key>
    <value>0</value>
    <type>real</type>
  </param>
  <param>
    <name>Rf Unit</name>
    <key>rf_unit</key>
    <value>1000000</value>
    <type>int</type>
    <option>
      <name>Hz</name>
      <key>1</key>
    </option>
    <option>
      <name>kHz</name>
      <key>1000</key>
    </option>
    <option>
      <name>MHz</name>
      <key>1000000</key>
    </option>
    <option>
      <name>GHz</name>
      <key>1000000000</key>
    </option>
  </param>
  <param>
    <name>GUI Hint</name>
    <key>gui_hint</key>
    <value></value>
    <type>gui_hint</type>
    <hide>part</hide>
  </param>
  <sink>
    <name>in</name>
    <type>float</type>
    <vlen>$fft_len</vlen>
  </sink>
  <sink>
    <name>map_in</name>
    <type>message</type>
  </sink>
  <source>
    <name>map_out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
