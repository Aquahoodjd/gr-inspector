<?xml version="1.0"?>
<block>
  <name>Fixed Resampler</name>
  <key>inspector_fixed_resampler_ff</key>
  <category>[Inspector]/Conditioning</category>
  <import>import inspector</import>
  <make>inspector.fixed_resampler_ff($signal, $out_samp_rate)</make>
  <callback>set_resamp_ratio($resamp_ratio)</callback>
  <param>
    <name>Type</name>
    <key>type</key>
    <type>enum</type>
    <option>
      <name>Complex</name>
      <key>complex</key>
      <opt>fcn:cc</opt>
    </option>
    <option>
      <name>Float</name>
      <key>float</key>
      <opt>fcn:ff</opt>
    </option>
  </param>
  <param>
    <name>Signal No</name>
    <key>signal</key>
    <value>0</value>
    <type>int</type>
  </param>
  <param>
    <name>Output Samp rate</name>
    <key>out_samp_rate</key>
    <value>48000</value>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>$type</type>
  </sink>
  <sink>
    <name>rate</name>
    <type>float</type>
    <optional>1</optional>
  </sink>
  <sink>
    <name>map_in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>out</name>
    <type>$type</type>
  </source>
</block>
